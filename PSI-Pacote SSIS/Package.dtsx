        <DTS:Executable
          DTS:refId="Package\Contêiner do Loop Foreach para a Pasta Fonte\Confere em Tabela do SQL, se arquivo já foi carregado"
          DTS:CreationName="Microsoft.ExecuteSQLTask"
          DTS:Description="Tarefa Executar SQL"
          DTS:DTSID="{01570850-ED6E-4817-818E-0DE430A4A640}"
          DTS:ExecutableType="Microsoft.ExecuteSQLTask"
          DTS:LocaleID="-1"
          DTS:ObjectName="Confere em Tabela do SQL, se arquivo já foi carregado"
          DTS:ThreadHint="0">
          <DTS:Variables />
          <DTS:PropertyExpression
            DTS:Name="SqlStatementSource">"DECLARE @FileName VARCHAR(100)
SET @FileName='"+ @[User::FileName] +"'
IF EXISTS (SELECT 1
           FROM   [dbo].[FileLoadStatus]
           WHERE  filename =@FileName)
  BEGIN
      SELECT 1 AS FileExistsFlg
  END
ELSE
BEGIN
Select 0 AS FileExistsFlg
END"</DTS:PropertyExpression>
